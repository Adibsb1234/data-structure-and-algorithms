#include<stdio.h>

int main(){

int arr[3][3];
int i,j,sum=0;
printf("enter the elements of array \n");
       for(i=0;i<3;i++){
       for(j=0;j<3;j++){
       printf("enter elements in [%d][%d] location\n",i,j);
	  scanf("%d",&arr[i][j]);
	  }}
	  printf("The elememts in a matrix is \n");
	   for(i=0;i<3;i++){
       for(j=0;j<3;j++){
       printf(" %d ",arr[i][j]);
       }
       printf("\n");
       }
	  printf("The sum of diagonal elements of matrix is \n");
	   for(i=0;i<3;i++){
       for(j=0;j<3;j++){
	     if(arr[i]==arr[j]){
	     sum+=arr[i][j];
	     }
	     else{
	     continue;
	     }}           }
	     printf("\nsum=%d",sum);
   return 0;
   }
